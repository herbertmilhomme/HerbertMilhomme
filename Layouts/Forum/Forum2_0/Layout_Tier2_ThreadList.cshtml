@{ 
    var db = Database.Open("ColonielHeights");
    IEnumerable<dynamic> threads=null;
    IEnumerable<dynamic> stickie=null;
	DateTime lastviewed = new DateTime();
    int discussion=0; //= UrlData[0];
    var forumname = "Forum Name";
	bool subscribe = false;
	string threadurl = UrlData[0];
    if(threadurl != null && threadurl != ""){
        if(int.TryParse(threadurl, out discussion) || int.TryParse(threadurl.Substring(0,threadurl.IndexOf("-")), out discussion)){
            if(discussion < 1){
                discussion = 1;
            }
            //tag_counter = discussion * numperpg;
            threads = db.Query("SELECT *, userprofile.alias as authoralias FROM website_forumthread FULL JOIN userprofile ON website_forumthread.author=userprofile.userid WHERE forumid=@0 AND remove='false'", discussion);
            forumname = db.QueryValue("SELECT name FROM website_forumdiscussion WHERE forumid=@0",discussion);
