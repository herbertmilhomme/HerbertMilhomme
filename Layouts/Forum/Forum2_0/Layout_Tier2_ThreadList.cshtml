        }else{
            //Response.Redirect("?SearchQuery=" + SearchQuery + "/" + discussion);
            /*threads = db.Query("SELECT * FROM website_forumdiscussion as d, website_forumthread as t WHERE d.name=@0 AND t.Forumid=d.forumid AND t.[remove]='false'",UrlData[0]);
            forumname = db.QueryValue("SELECT name FROM website_forumdiscussion WHERE name=@0",UrlData[0]);*/

			discussion = db.QueryValue("SELECT coalesce(forumid, 0) as forumid FROM website_forumdiscussion WHERE LOWER(name) LIKE LOWER(@0)",(string)threadurl.Replace("-"," "));
            forumname = db.QueryValue("SELECT name FROM website_forumdiscussion WHERE forumid=@0",discussion);//UrllData[0] or ForumID//threadurl.Replace("-"," ");//
			if(discussion==0){threads = db.Query("SELECT *, userprofile.alias as authoralias FROM website_forumthread FULL JOIN userprofile ON website_forumthread.author=userprofile.userid WHERE remove='false'"); forumname = "All Discussions";}
			else{threads = db.Query("SELECT *, userprofile.alias as authoralias FROM website_forumthread FULL JOIN userprofile ON website_forumthread.author=userprofile.userid WHERE forumid=@0 AND remove='false'", discussion);}            
        }
		if(WebSecurity.IsAuthenticated){
			// Check if subscribe
			subscribe = db.QueryValue("SELECT subscribe FROM website_forumdiscussionview WHERE forumid=@0 AND userid=@1", discussion, WebSecurity.CurrentUserId)??false;
			if (subscribe == null) {
				// Insert into view table
				db.Execute("INSERT INTO website_forumdiscussionview (lastview, forumid, userid, viewed) VALUES (@0, @1, @2, @3)", DateTime.Now,discussion,WebSecurity.CurrentUserId, true);
			}else{db.Execute("UPDATE website_forumdiscussionview SET lastview=@0 WHERE forumid=@1 AND userid=@2",DateTime.Now, discussion, WebSecurity.CurrentUserId);}
		}
    }else{threads = db.Query("SELECT *, userprofile.alias as authoralias FROM website_forumthread FULL JOIN userprofile ON website_forumthread.author=userprofile.userid WHERE remove='false'"); forumname = "All Discussions";}
	//int replies = 0;
    //IEnumerable<dynamic> lastpost = null;

    int numperpg = /*int.Parse(PageData[0]) ?? Request.Cookies["perpage"] ?? int.Parse(Request.QueryString["perpage"]) ?? int.Parse(Request.Form["perpage"]) ??*/ 20;
	int totalthreads = threads==null?0:threads.Count();
	int lastcount = 0;
    int currentpage = threads==null? 1 : (int)Math.Ceiling((decimal)totalthreads / numperpg);//1;
    int tag_counter = currentpage==1? 1:currentpage * numperpg;//*0;
    if(Request.QueryString["page"] != null){
        if(int.TryParse(Request.QueryString["page"], out currentpage)){
            currentpage = (int)Request.QueryString["page"].AsInt();
            if(currentpage >= (threads.Count() / numperpg)){
                currentpage = threads.Count() / numperpg;
            }
            if(currentpage < 1){
                currentpage = 1;
            }
            tag_counter = currentpage==1? 1:currentpage * numperpg;
        }else{
            Response.Redirect("?page=" + currentpage);
        }
    }
	Html.Hidden("TotalTagCount",totalthreads);
	var subForum = db.Query("SELECT * FROM website_forumdiscussion WHERE TierTree=@0 ORDER BY name", discussion);
