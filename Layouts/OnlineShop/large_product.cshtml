<div itemscope itemtype="http://schema.org/Product" class="shop-product-large clear">   
    <div class="details">   
        <h1 itemprop="name">{tag_name}</h1>  
        <ul itemprop="offers" itemscope itemtype="http://schema.org/Offer">	   
			<meta itemprop="priceCurrency" content="USD" />
            <!--<div class="price">if(item is on sale || item is clearance){
				<div class="price_new">$<span itemprop="price" content="1000.00">9.95</span>
				(Sale ends <time itemprop="priceValidUntil" datetime="2020-11-05">
      5 November!</time>)
			</div>
				<div class="price_old">$<span itemprop="price" style="text-decoration:line-through;">19.95</span></div>}else{<strong><span itemprop="price" content="1000.00">{tag_saleprice}</span></strong>}
			</div>-->
            <li class="price"><strong><span itemprop="price" content="1000.00">{tag_saleprice}</span></strong></li>   
            <li class="instock">
      <meta itemprop="itemCondition" itemtype="http://schema.org/OfferItemCondition" content="http://schema.org/NewCondition"/>
				if(==0){<meta itemprop="availability" content="http://schema.org/InStock"/><strong><span class="OOS">Out of Stock!</span></strong>}else{<meta itemprop="availability" content="http://schema.org/InStock"/>In stock: <strong>tag_instock</strong>}</li>   
            <li class="quantity">{tag_addtocartinputfield} <img src="/CatalystImages/shop_addtocart.png" alt="Add to cart" /></li> <!--BUTTON-->   
        </ul>   
        <div class="description">
			<meta itemprop="sku" content="abc123" /> 
			<meta itemprop="gtin14" content="00886227537143" />if(item==package){foreach(gtin in package){Package contains inside:QTY META SKU<br/>}}
			{tag_description}
			<!--
			**Product description:**
  <span itemprop="description">0.7 cubic feet countertop microwave.
  Has six preset cooking categories and convenience features like
  Add-A-Minute and Child Lock.</span>
  **Customer reviews:**
  <div itemprop="review" itemscope itemtype="http://schema.org/Review">
    <span itemprop="name">Not a happy camper</span> -
    by <span itemprop="author">Ellie</span>,
    <meta itemprop="datePublished" content="2011-04-01">April 1, 2011
    <div itemprop="reviewRating" itemscope itemtype="http://schema.org/Rating">
      <meta itemprop="worstRating" content = "1">
      <span itemprop="ratingValue">1</span>/
      <span itemprop="bestRating">5</span>stars
    </div>
    <span itemprop="description">The lamp burned out and now I have to replace
    it. </span>
  </div>
			-->
		</div>   
    </div>   
    <div class="image">   
        {tag_largeimage}   
        <div class="poplets">@RenderPage("~/Layouts/OnlineShop/poplets.cshtml")</div>   
    </div>   
</div><!--      
